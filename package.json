{
  "name": "stats",
  "scripts": {
    "start": "nodemon -e js,graphql -x node -r dotenv/config server.js",
    "debug": "nodemon -e js,graphql -x node --inspect -r dotenv/config server.js",
    "playground": "graphql playground",
    "dev": "npm-run-all --parallel start playground",
    "test": "jest",
    "test-watch": "jest --watch",
    "dcr": "docker-compose up -d",
    "codecov": "jest --coverage && codecov",
    "new_issuer_key": "scripts/create_issuer_key.sh",
    "db-migrate": "db-migrate",
    "prettier": "prettier",
    "prettier-all": "prettier --write 'src/**/*.js' '*.js' '*.json' 'config/*.js' 'config/*.json'"
  },
  "dependencies": {
    "apollo-server-express": "^1.3.6",
    "babel-core": "^6.26.3",
    "bcryptjs": "2.4.3",
    "body-parser-graphql": "^1.1.0",
    "config": "^1.30.0",
    "cron": "^1.3.0",
    "db-migrate": "^0.11.1",
    "db-migrate-pg": "^0.4.0",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "graphql-auth": "^0.2.2",
    "graphql-tag": "^2.9.1",
    "graphql-tester": "^0.0.5",
    "graphql-tools": "^3.0.4",
    "jsonwebtoken": "^8.2.1",
    "maxmind": "^2.7.0",
    "pg": "^7.4.3"
  },
  "devDependencies": {
    "babel-plugin-import-graphql": "^2.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "codecov": "^3.0.2",
    "dotenv": "5.0.1",
    "jest": "^22.4.3",
    "jest-babel": "^1.0.1",
    "nodemon": "^1.17.5",
    "npm-run-all": "4.1.2",
    "prettier": "^1.13.5"
  }
}
